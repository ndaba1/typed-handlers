{
  "name": "typed-handlers",
  "version": "0.0.2",
  "description": "Type-safe REST APIs in Next.js",
  "exports": {
    "./routes": {
      "types": "./routes.d.ts"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "module": "./dist/next.js",
      "require": "./dist/next.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "keywords": [
    "next",
    "zod",
    "typed-handlers",
    "typescript"
  ],
  "author": "Victor Ndaba <mwangindaba@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "next": ">= 13",
    "zod": ">= 3.22"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "ndaba1/typed-handlers",
  "homepage": "https://typed-handlers.vercel.app"
}
